<script>
  "use strict";

  const good = 'Apple';
  console.log(getGoodPrice(good));
  console.log(getGoodPrice("Mango"));
  
  function getGoodPrice(trade) {
    switch (trade) {
      case 'Banana':
        return 50;
      case 'Mango':
        return 80;
      case 'Apple':
        return 40;
      default:
        return NaN;
    }
}
</script>
